<main>
<h1>Books</h1>

<table>
  <tbody>
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th colspan="3"></th>
    </tr>
  </tbody>
</table>

<h2>New books</h2>

<%= form_with model:@book,local:true do |f| %>

<p>Title</p>
<%= f.text_field :title %>

<p>Body</p>
<%= f.text_area :body %>

<%= f.submit 'Create Book' %>

<% end %>
</main>
